# create or move into your project folder
mkdir ai-transport-demand-model
cd ai-transport-demand-model

# initialize it as a local git repository
git init

# connect (link) it to your GitHub repo
git remote add origin https://github.com/yourusername/ai-transport-demand-model.git

# create at least one file (e.g., README)
echo "# AI Transport Demand Model" > README.md

# add and commit
git add .
git commit -m "Initial commit"

# push to GitHub
git branch -M main
git push -u origin main
